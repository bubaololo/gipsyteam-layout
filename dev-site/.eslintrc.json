{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "globals": {
    "$": true
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "max-len": "off",
    "no-var": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
